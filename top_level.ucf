NET  "d<11>"              LOC="AN14" | IOSTANDARD = LVCMOS33;

NET d<*>  IOSTANDARD = LVDCI_33;

NET "blank"					LOC = "AE8"  | IOSTANDARD = LVDCI_33;  #data enable

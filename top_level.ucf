NET cpu_pc_MSB LOC="AE26";  # switch 6
NET mem_sys_fin LOC="AE27";  # switch 5

# state output
NET state<0> LOC="AD26";  # led3(rxd)
NET state<1> LOC="G15";  # led2(rxd)

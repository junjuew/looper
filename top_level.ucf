#DVI output
NET  "d<0>"               LOC="AB8" | IOSTANDARD = LVCMOS33; 
NET  "d<1>"               LOC="AC8" | IOSTANDARD = LVCMOS33;
NET  "d<2>"               LOC="AN12" | IOSTANDARD = LVCMOS33;
NET  "d<3>"               LOC="AP12" | IOSTANDARD = LVCMOS33;
NET  "d<4>"               LOC="AA9" | IOSTANDARD = LVCMOS33;
